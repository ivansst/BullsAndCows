@model BullsAndCows.Models.UserTriesModel

@{
    ViewData["Title"] = "Scoreboard";
}
<h1>@ViewData["Scoreboard"]</h1>


<table class="table">
    <thead>
        <tr>
            <th scope="col">User:</th>
            <th scope="col">Has beaten the game in:</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model.UserTries)
        {
            <tr>
                <td scope="row">@user.UserName</td>
                <td scope="row">@user.Tries tries</td>
            </tr>

        }
    </tbody>
</table>

